{
  "expo": {
	"name": "Medicina Estética",
	"slug": "medicina-estetica",
	"version": "1.0.0",
	"orientation": "portrait",
	"icon": "./assets/icon.png",
	"userInterfaceStyle": "light",
	"splash": {
  	"image": "./assets/splash.png",
  	"resizeMode": "contain",
  	"backgroundColor": "#2196F3"
	},
	"assetBundlePatterns": [
  	"**/*"
	],
	"ios": {
  	"supportsTablet": true,
  	"bundleIdentifier": "com.estetica.cordoba"
	},
	"android": {
  	"adaptiveIcon": {
    	"foregroundImage": "./assets/adaptive-icon.png",
    	"backgroundColor": "#2196F3"
  	},
  	"package": "com.estetica.cordoba",
  	"permissions": [
    	"CAMERA",
    	"WRITE_EXTERNAL_STORAGE",
    	"READ_EXTERNAL_STORAGE",
    	"RECORD_AUDIO"
  	]
	},
	"web": {
  	"favicon": "./assets/favicon.png",
  	"name": "Medicina Estética - Córdoba",
  	"shortName": "MedEstética",
  	"lang": "es",
  	"scope": "/",
  	"themeColor": "#2196F3"
	},
	"extra": {
  	"apiUrl": "http://localhost:3000/api",
  	"environment": "development"
	},
	"plugins": [
  	"expo-camera",
  	"expo-image-picker",
  	"expo-document-picker",
  	"expo-calendar",
  	"expo-notifications"
	]
  }
}
