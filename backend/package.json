{
  "name": "estetica-backend",
  "version": "1.0.0",
  "description": "Backend API para sistema de gestión de medicina estética",
  "main": "dist/server.js",
  "scripts": {
	"build": "tsc",
	"start": "node dist/server.js",
	"dev": "ts-node-dev --respawn --transpile-only src/server.ts",
	"dev:debug": "ts-node-dev --inspect --respawn --transpile-only src/server.ts",
	"prisma:generate": "prisma generate",
	"prisma:push": "prisma db push",
	"prisma:migrate": "prisma migrate dev",
	"prisma:migrate:prod": "prisma migrate deploy",
	"prisma:studio": "prisma studio",
	"prisma:seed": "ts-node src/scripts/seed.ts",
	"create-admin": "ts-node src/scripts/createAdmin.ts",
	"create-admin:default": "ts-node src/scripts/createAdmin.ts --default",
	"test": "jest",
	"test:watch": "jest --watch",
	"test:coverage": "jest --coverage",
	"lint": "eslint src/**/*.ts",
	"lint:fix": "eslint src/**/*.ts --fix",
	"format": "prettier --write src/**/*.ts",
	"clean": "rimraf dist",
	"prestart": "npm run build",
	"postinstall": "npm run prisma:generate"
  },
  "keywords": [
	"medicina-estetica",
	"api",
	"nodejs",
	"typescript",
	"prisma",
	"postgresql",
	"jwt",
	"authentication",
	"cordoba",
	"argentina"
  ],
  "author": "Medicina Estética - Córdoba",
  "license": "MIT",
  "dependencies": {
	"@prisma/client": "^5.14.0",
	"express": "^4.19.2",
	"cors": "^2.8.5",
	"helmet": "^7.1.0",
	"morgan": "^1.10.0",
	"bcryptjs": "^2.4.3",
	"jsonwebtoken": "^9.0.2",
	"joi": "^17.12.3",
	"multer": "^1.4.5-lts.1",
	"nodemailer": "^6.9.13",
	"axios": "^1.6.8",
	"moment": "^2.30.1",
	"moment-timezone": "^0.5.45",
	"googleapis": "^134.0.0",
	"google-auth-library": "^9.0.0",
	"mercadopago": "^2.0.11",
	"express-rate-limit": "^7.2.0",
	"express-validator": "^7.0.1",
	"compression": "^1.7.4",
	"dotenv": "^16.4.5",
	"uuid": "^9.0.1",
	"sharp": "^0.33.3",
	"csv-parser": "^3.0.0",
	"express-fileupload": "^1.5.0",
	"winston": "^3.13.0",
	"winston-daily-rotate-file": "^5.0.0",
	"node-cron": "^3.0.3"
  },
  "devDependencies": {
	"@types/express": "^4.17.21",
	"@types/cors": "^2.8.17",
	"@types/morgan": "^1.9.9",
	"@types/bcryptjs": "^2.4.6",
	"@types/jsonwebtoken": "^9.0.6",
	"@types/multer": "^1.4.11",
	"@types/nodemailer": "^6.4.15",
	"@types/compression": "^1.7.5",
	"@types/uuid": "^9.0.8",
	"@types/node": "^20.12.7",
	"@types/jest": "^29.5.12",
	"@types/readline": "^1.3.3",
"@types/node-cron": "^3.0.8",
"@typescript-eslint/eslint-plugin": "^7.8.0",
	"@typescript-eslint/parser": "^7.8.0",
	"eslint": "^8.57.0",
	"eslint-config-prettier": "^9.1.0",
	"eslint-plugin-prettier": "^5.1.3",
	"prettier": "^3.2.5",
	"jest": "^29.7.0",
	"ts-jest": "^29.1.2",
	"supertest": "^7.0.0",
	"@types/supertest": "^6.0.2",
	"ts-node": "^10.9.2",
	"ts-node-dev": "^2.0.0",
	"typescript": "^5.4.5",
	"prisma": "^5.14.0",
	"rimraf": "^5.0.7"
  },
  "engines": {
	"node": ">=18.0.0",
	"npm": ">=9.0.0"
  },
  "repository": {
	"type": "git",
	"url": "git+https://github.com/usuario/estetica-app.git"
  },
  "bugs": {
	"url": "https://github.com/usuario/estetica-app/issues"
  },
  "homepage": "https://github.com/usuario/estetica-app#readme"
}










